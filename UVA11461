//UVA11461 Square Numbers
import java.util.*;
public class UVA11461{
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        while(sc.hasNext()){
        	int a=sc.nextInt();
        	int b=sc.nextInt();
        	int count=0;
        	if(a==0 || b==0)
        		break;
        	for(int i=a;i<=b;i++){
        		if(Math.sqrt(i)%1==0)
        			count++;
        	}
        	System.out.println(count);
        }
    }
};
