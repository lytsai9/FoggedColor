//UVA10071 Back to high school physics
import java.util.*;
public class UVA10071{
    public static void main(String[] args) {
        Scanner sc=new Scanner (System.in);
        while(sc.hasNext()){
        int a=sc.nextInt();
        int b=sc.nextInt();
        System.out.println(a*b*2);
        }
    }
};
